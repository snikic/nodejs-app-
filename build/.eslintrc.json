{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "es6": true
    },
    "plugin": [
        "security"
    ],
    "rules": {
        "comma-dangle": [2, "never"],
        "no-cond-assign": [2, "always"],
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 1,
        "no-empty": 1,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [2, "all", {"returnAssign": false}],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [2, "both"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": [1, {
            "prefer": {
                "return": "return"
            },
            "requireReturn": false,
            "requireParamDescription": false
        }],
        "valid-typeof": 2,


        "accessor-pairs": 1,
        "block-scoped-var": 2,
        "complexity": [1, 6],
        "consistent-return": 2,
        "curly": [2, "multi-line", "consistent"],
        "default-case": 1,
        "dot-location": [2, "property"],
        "dot-notation": 2,
        "eqeqeq": [1, "smart"],
        "guard-for-in": 1,
        "no-alert": 2,
        "no-caller": 2,
        "no-div-regex": 1,
        "no-else-return": 2,
        "no-empty-pattern": 1,
        "no-eq-null": 1,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 1,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 1,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 1,
        "no-labels": 2,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-magic-numbers": [1, {
            "enforceConst": true,
            "ignoreArrayIndexes": true,
            "ignore": [-1, 0, 1, 2, 3, 4, 5, 30, 60, 100, 200, 400, 500, 1000]
        }],
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 0,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-param-reassign": 2,
        "no-process-env": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": [2, "except-parens"],
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 1,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-void": 2,
        "no-warning-comments": [1, {"terms": ["todo", "fixme", "implementme"], "location": "start"}],
        "no-with": 2,
        "radix": 1,
        "vars-on-top": 0,
        "wrap-iife": [2, "any"],
        "yoda": [1, "never"],


        "strict": [2, "global"],


        "init-declarations": 0,
        "no-catch-shadow": 0,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 1,
        "no-undef-init": 2,
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
        "no-use-before-define": 2,


        "callback-return": [2, ["callback", "next", "respond"]],
        "global-require": 2,
        "handle-callback-err": [2, "^.*(e|E)rr"],
        "no-mixed-requires": 2,
        "no-new-require": 2,
        "no-path-concat": 2,
        "no-process-exit": 2,
        "no-restricted-modules": 0,
        "no-sync": 1,


        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "always"],
        "brace-style": [1, "1tbs", {"allowSingleLine": true}],
        "camelcase": 1,
        "comma-spacing": [1, {"before": false, "after": true}],
        "comma-style": [1, "last"],
        "computed-property-spacing": [2, "never"],
        "consistent-this": 0,
        "eol-last": 1,
        "func-names": 0,
        "func-style": [2, "expression", { "allowArrowFunctions": true }],
        "id-length": [2, {"min": 2, "max": 25, "properties": "always", "exceptions": ["x", "y", "i", "$"]}],
        "id-match": 0,
        "indent": [1, 4, {"VariableDeclarator": {"var": 2, "let": 2, "const": 3}, "SwitchCase": 1}],
        "jsx-quotes": 0,
        "linebreak-style": [2, "unix"],
        "lines-around-comment": 0,
        "key-spacing": [1, {"beforeColon": false, "afterColon": true}],
        "max-nested-callbacks": [1, 3],
        "new-cap": [2, {"newIsCap": true, "capIsNew": true}],
        "new-parens": 2,
        "newline-after-var": 0,
        "no-array-constructor": 2,
        "no-continue": 1,
        "no-inline-comments": 0,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": [1, {"max": 2}],
        "no-negated-condition": 1,
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-restricted-syntax": 0,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 1,
        "object-curly-spacing": [2, "never"],
        "one-var": 0,
        "operator-assignment": [1, "always"],
        "operator-linebreak": [1, "before"],
        "padded-blocks": [2, "never"],
        "quote-props": [1, "always"],
        "quotes": [2, "single"],
        "require-jsdoc": 0,
        "semi-spacing": [2, {"before": false, "after": true}],
        "semi": [2, "always"],
        "sort-vars": 0,
        "keyword-spacing": [1, {"before": true, "after": true}],
        "space-before-blocks": [1, "always"],
        "space-before-function-paren": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-unary-ops": [2, {"words": true, "nonwords": false}],
        "spaced-comment": [1, "always", { "exceptions": ["-"] }],
        "wrap-regex": 1,


        "arrow-parens": [2, "as-needed"],
        "arrow-spacing": [2, {"before": true, "after": true}],
        "constructor-super": 2,
        "generator-star-spacing": [2, "before"],
        "no-class-assign": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2,
        "no-var": 1,
        "object-shorthand": [1, "always"],
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-reflect": 1,
        "prefer-spread": 1,
        "prefer-template": 2,
        "require-yield": 1,


        "no-bitwise": 1,
        "max-params": [1, 4]
    }
}
